{
  "name": "fullstack-react-serverless",
  "version": "0.1.0",
  "repository": "https://github.com/tim-stasse/fullstack-react-serverless.git",
  "author": "Tim Stasse <tjstasse@agiledigital.com.au>",
  "license": "MIT",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.1",
    "cross-env": "^5.2.0",
    "eslint": "5.6.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^4.7.1"
  },
  "scripts": {
    "audit-all": "run-p audit:*",
    "audit:admin": "cd ./admin && yarn audit",
    "audit:client": "cd ./client && yarn audit",
    "audit:self": "yarn audit",
    "audit:server": "cd ./server && yarn audit",
    "build-all": "run-p build:*",
    "build:admin": "cd ./admin && yarn build",
    "build:client": "cd ./client && yarn build",
    "build:server": "cd ./server && yarn package",
    "install-all": "yarn install && run-s install:*",
    "install:admin": "cd ./admin && yarn install",
    "install:client": "cd ./client && yarn install",
    "install:server": "cd ./server && yarn install",
    "lint-staged": "lint-staged",
    "start-all": "run-p start:*",
    "start:admin": "cd ./admin && yarn start",
    "start:client": "cd ./client && yarn start",
    "start:server": "cd ./server && yarn start",
    "test-all": "run-p test:*",
    "test:admin": "cd ./admin && yarn test",
    "test:client": "cd ./client && yarn test"
  }
}
